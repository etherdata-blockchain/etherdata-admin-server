{
  "name": "etd-admin-ui",
  "version": "1.8.1",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "dev": "ts-node --project tsconfig.server.json server/app.ts",
    "start": "NODE_ENV=production node --max_old_space_size=8192 dist/app.js",
    "lint": "next lint",
    "test": "jest --forceExit --env=jsdom",
    "release": "standard-version",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/lab": "^5.0.0-alpha.55",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "@mui/x-data-grid": "^5.0.1",
    "@types/css-mediaquery": "^0.1.1",
    "@types/dockerode": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/randomcolor": "^0.5.6",
    "@types/uuid": "^8.3.1",
    "axios": "^0.21.1",
    "bootstrap": "5.1.0",
    "bson": "^4.5.2",
    "css-mediaquery": "^0.1.2",
    "cypress": "^9.0.0",
    "dockerode": "^3.3.1",
    "etd-react-ui": "^2.0.1",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "material-ui-popup-state": "^2.0.0",
    "moment": "^2.29.1",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.13",
    "next": "12.0.1",
    "nextjs-progressbar": "^0.0.11",
    "randomcolor": "^0.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "realm-web": "^1.3.0",
    "recharts": "^2.0.9",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "start-server-and-test": "^1.14.0",
    "styled-components": "^5.3.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.1.0",
    "uuid": "^8.3.2",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.14.5",
    "@shelf/jest-mongodb": "^2.0.1",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-react": "^7.27.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.4.4",
    "yaml": "^1.10.2"
  },
  "standard-version": {
    "packageFiles": [
      {
        "filename": "k8s/deployment.yml",
        "updater": "k8s/update-version.js"
      },
      {
        "filename": "package.json"
      }
    ],
    "bumpFiles": [
      {
        "filename": "k8s/deployment.yml",
        "updater": "k8s/update-version.js"
      },
      {
        "filename": "package.json"
      }
    ]
  }
}
